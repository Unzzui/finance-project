{
    "version": 2,
    "builds": [
      {
        "src": "finance_app/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.12" }
      }

    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "finance_app/wsgi.py"
      }
    ]
  }
  
  